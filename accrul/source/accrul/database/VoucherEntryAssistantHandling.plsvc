@Override
PROCEDURE CRUD_Create___(
   rec_ IN Voucher_Entry_Rec )
IS
   local_rec_          Voucher_Entry_Rec;     
   temp_info_          VARCHAR2(4000);
   temp_obj_id_        VARCHAR2(4000); 
   temp_obj_version_   VARCHAR2(4000);
   attr_               VARCHAR2(32000);
   voucher_head_info_rec_  Voucher_Header_Info_Structure_Rec;
BEGIN   
   IF (rec_.openned_from = 'NEW_VOUCHER') THEN
      local_rec_ := rec_;
     
      local_rec_.voucher_date := NVL(rec_.voucher_date,trim(sysdate));
      Add_To_Attr_From_Rec___(local_rec_,attr_);
      Voucher_API.New__(temp_info_,
                        temp_obj_id_,
                        temp_obj_version_,
                        attr_,
                        'PREPARE');
                                                 
      Add_To_Rec_From_Attr___(attr_,local_rec_);    
      
   ELSE
      local_rec_ := rec_;
      Copy_Hold_Voucher___(local_rec_);  
   END IF;
   voucher_head_info_rec_ := Voucher_Header_Info___ (local_rec_.company, local_rec_.accounting_year, local_rec_.user_group, local_rec_.voucher_type);
   --  new_.automatic := voucher_head_info_rec_.automatic;
   -- new_.function_group := voucher_head_info_rec_.function_group;
   local_rec_.simulation_voucher := voucher_head_info_rec_.simulation_voucher;
   -- new_.use_apporval_workflow := voucher_head_info_rec_.use_apporval_workflow;
   -- new_.separate_user_approval := voucher_head_info_rec_.separate_user_approval;
   super(local_rec_);
   
   IF (rec_.openned_from = 'COPY_VOUCHER') THEN
      Copy_Hold_Voucher_Rows___(local_rec_);
   END IF;   
END CRUD_Create___;

