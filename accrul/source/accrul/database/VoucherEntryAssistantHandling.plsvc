PROCEDURE Trace_Stmnt (
   stmnt_        IN VARCHAR2 )
IS
   stmntx_          VARCHAR2(32000);
   stmnty_          VARCHAR2(32000);
   instr_           NUMBER;
   line_sign_       VARCHAR2(1)  := CHR(10);
BEGIN
   stmntx_ := stmnt_;
   stmntx_ := REPLACE(stmntx_,' FROM ',' '||line_sign_||'FROM ');
   stmntx_ := REPLACE(stmntx_,' WHERE ',' '||line_sign_||'WHERE ');
   stmntx_ := REPLACE(stmntx_,' ORDER BY ',' '||line_sign_||'ORDER BY ');
   Trace_SYS.Message ('***** Start: The dynamic SQL statement look like this *****');
   LOOP
      IF (LENGTH(stmntx_) <= 200) THEN
         Trace_SYS.Message (stmntx_);
         EXIT;
      END IF;
      instr_  := INSTR(stmntx_,line_sign_);
      IF (instr_ > 200 OR NVL(instr_,0) = 0) THEN
         instr_ := 200;
      END IF;
      stmnty_ := SUBSTR(stmntx_,1,instr_);
      stmnty_ := REPLACE(stmnty_,line_sign_,' ');
      stmntx_ := SUBSTR(stmntx_,instr_+1);
      Trace_SYS.Message (stmnty_);
   END LOOP;
   Trace_SYS.Message ('***** End: The dynamic SQL statement look like this *****');   
END Trace_Stmnt;


PROCEDURE Validate_Voucher_Posting___ (
	init_ IN BOOLEAN)
IS
   tax_percentage_                     NUMBER;
   dummy_                              NUMBER;
   total_amount_                       NUMBER := 0;
   total_tax_amount_                   NUMBER := 0;
   total_currency_amount_              NUMBER:= 0;
   total_currency_tax_amount_          NUMBER:= 0;
   total_parallel_currency_amount_     NUMBER := 0;
   total_parallel_currency_tax_amount_ NUMBER := 0;
   null_amount_key_                    VARCHAR2(50);
   voucher_entry_rec_                  Voucher_Entry_Rec;
   voucher_entry_key_                  Voucher_Entry_Key;
   voucher_posting_rec_                Voucher_posting_Rec;
   voucher_posting_key_                Voucher_posting_Key;
   new_voucher_posting_rec_            Voucher_posting_Rec;
   base_currency_code_                 voucher_row_tab.currency_code%TYPE;
   
   company_rec_ Company_Finance_API.Public_rec;
   CURSOR get_sum_amounts IS
   SELECT objkey,
          parent_objkey,
          currency_code,
          company,
          account,
          optional_code,
          (nvl(debet_amount,0) - nvl(credit_amount,0)) sum_amount,
          (nvl(tax_amount,0)) sum_tax_amount,
          (nvl(currency_debet_amount,0) - nvl(currency_credit_amount,0)) sum_currency_amount,
          (nvl(currency_tax_amount,0) ) sum_currency_tax_amount,
          (nvl(third_currency_debit_amount,0) - nvl(third_currency_credit_amount,0)) sum_third_currency_amount,
          (nvl(parallel_curr_tax_amount,0)) sum_third_currency_tax_amount     
      FROM ${PROJECTION}_voucher_posting_vrt
      WHERE parent_objkey = global_parent_objkey_;
BEGIN
   IF (init_) THEN
      global_parent_objkey_ := NULL;
   ELSE
      IF (global_parent_objkey_ IS NOT NULL ) THEN
         voucher_entry_key_.objkey := global_parent_objkey_;
         voucher_entry_rec_ := Get_Record___(voucher_entry_key_);
         IF (Voucher_Type_Detail_API.Get_Automatic_Vou_Balance(
                        voucher_entry_rec_.company,
                        voucher_entry_rec_.voucher_type,
                        voucher_entry_rec_.function_group)= 'Y') THEN
      
            company_rec_ := Company_Finance_API.Get(global_company_);
            base_currency_code_ := company_rec_.currency_code;
            FOR rec_ IN get_sum_amounts LOOP      
               IF (base_currency_code_ != rec_.currency_code) THEN
                  RETURN;      
               END IF;
               IF (NVL(Account_API.Get_Stat_Account_Db(rec_.company, rec_.account),'N') = 'N') THEN         
                  total_amount_ := total_amount_ + rec_.sum_amount;
                  total_tax_amount_ := total_tax_amount_ + rec_.sum_tax_amount;
                  total_currency_amount_ := total_currency_amount_ + rec_.sum_currency_amount;
                  total_currency_tax_amount_ := total_currency_tax_amount_ + rec_.sum_currency_tax_amount;
                  total_parallel_currency_amount_ := total_parallel_currency_amount_ + rec_.sum_third_currency_amount;
                  total_parallel_currency_tax_amount_ := total_parallel_currency_tax_amount_ + rec_.sum_third_currency_tax_amount;
               END IF;
               IF (rec_.sum_amount = 0) THEN
                  null_amount_key_ := rec_.objkey;
               END IF;
            END LOOP;

            IF (voucher_entry_rec_.amount_method = 'GROSS') THEN     
               total_tax_amount_                   := 0;
               total_currency_tax_amount_          := 0;
               total_parallel_currency_tax_amount_ := 0;
            END IF;
            IF (total_amount_ != 0 AND null_amount_key_ IS NOT NULL ) THEN      
               voucher_posting_key_.objkey := null_amount_key_;
               voucher_posting_rec_ := Get_Record___(voucher_posting_key_);
               new_voucher_posting_rec_ := voucher_posting_rec_;
               tax_percentage_ := Statutory_Fee_API.Get_Tax_Percentage(new_voucher_posting_rec_.company, new_voucher_posting_rec_.optional_code);
              
               new_voucher_posting_rec_.amount                 := -1 * (total_amount_ + total_tax_amount_);
               new_voucher_posting_rec_.currency_amount        := -1 * (total_currency_amount_ + total_currency_tax_amount_);
               new_voucher_posting_rec_.third_currency_amount  := -1 * (total_parallel_currency_amount_ + total_parallel_currency_tax_amount_);

               
               IF (voucher_posting_rec_.optional_code IS NOT NULL ) THEN
                  new_voucher_posting_rec_.tax_base_amount               := new_voucher_posting_rec_.amount;
                  new_voucher_posting_rec_.currency_tax_base_amount      := new_voucher_posting_rec_.currency_amount;
                  new_voucher_posting_rec_.parallel_curr_tax_base_amount := new_voucher_posting_rec_.third_currency_amount;
         
                  Tax_Handling_Accrul_Util_API.Calculate_Tax_Amounts(new_voucher_posting_rec_.currency_tax_amount ,
                                                                     dummy_,
                                                                     dummy_,
                                                                     new_voucher_posting_rec_.company,
                                                                     new_voucher_posting_rec_.Currency_Code,
                                                                     new_voucher_posting_rec_.optional_code,
                                                                     tax_percentage_,                                                                                              
                                                                     'FALSE',                                                                                            
                                                                     new_voucher_posting_rec_.amount,
                                                                     'GROSS',
                                                                     Currency_Code_API.Get_Currency_Rounding(new_voucher_posting_rec_.company, new_voucher_posting_rec_.Currency_Code),
                                                                     Tax_Handling_Util_API.Get_Tax_Rounding_Method_Db(new_voucher_posting_rec_.company, NULL ,NULL , NULL ),
                                                                     voucher_entry_rec_.voucher_date );


                  new_voucher_posting_rec_.parallel_curr_tax_amount := Execute_Parallel_Currency_Calculation___ ( 
                                                                        new_voucher_posting_rec_.company,
                                                                        company_rec_.parallel_acc_currency,              
                                                                        new_voucher_posting_rec_.tax_amount,
                                                                        new_voucher_posting_rec_.currency_tax_amount,
                                                                        company_rec_.parallel_base,
                                                                        new_voucher_posting_rec_.parallel_currency_rate,  
                                                                        Currency_Rate_API.Is_Parallel_Curr_Rate_Inverted(new_voucher_posting_rec_.company, new_voucher_posting_rec_.currency_code),
                                                                        new_voucher_posting_rec_.parallel_conversion_factor );


                  new_voucher_posting_rec_.tax_amount := new_voucher_posting_rec_.currency_tax_amount;
                  new_voucher_posting_rec_.parallel_curr_tax_amount := NVL(new_voucher_posting_rec_.parallel_curr_tax_amount,0);
                  IF (voucher_entry_rec_.amount_method = 'NET') THEN
                     new_voucher_posting_rec_.currency_amount        := new_voucher_posting_rec_.currency_amount - NVL(new_voucher_posting_rec_.currency_tax_amount,0);
                     new_voucher_posting_rec_.amount                 := new_voucher_posting_rec_.amount - NVL(new_voucher_posting_rec_.tax_amount,0);
                     new_voucher_posting_rec_.third_currency_amount  := new_voucher_posting_rec_.third_currency_amount - NVL(new_voucher_posting_rec_.parallel_curr_tax_amount,0);

                  END IF;
               END IF;
               IF (new_voucher_posting_rec_.currency_amount > 0 ) THEN
                  new_voucher_posting_rec_.debet_amount := new_voucher_posting_rec_.amount;
                  new_voucher_posting_rec_.currency_debet_amount := new_voucher_posting_rec_.currency_amount;
                  new_voucher_posting_rec_.third_currency_debit_amount := new_voucher_posting_rec_.third_currency_amount;
                  new_voucher_posting_rec_.credit_amount := NULL;
                  new_voucher_posting_rec_.currency_credit_amount := NULL;
                  new_voucher_posting_rec_.third_currency_credit_amount := NULL;
               ELSE
                  new_voucher_posting_rec_.credit_amount := -1 * new_voucher_posting_rec_.amount;
                  new_voucher_posting_rec_.currency_credit_amount := -1 * new_voucher_posting_rec_.currency_amount;
                  new_voucher_posting_rec_.third_currency_credit_amount := -1 * new_voucher_posting_rec_.third_currency_amount;
                  new_voucher_posting_rec_.debet_amount := NULL;
                  new_voucher_posting_rec_.currency_debet_amount := NULL;
                  new_voucher_posting_rec_.third_currency_debit_amount := NULL;  
               END IF;
               CRUD_Update___(voucher_posting_rec_, new_voucher_posting_rec_);
            END IF;
         END IF;
      END IF;
	END IF;
END Validate_Voucher_Posting___;


