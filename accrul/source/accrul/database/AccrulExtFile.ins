         AND    column_id        = column_id_;
      CURSOR get_rowno IS
         SELECT NVL(MAX(row_no),0)
         FROM   ext_file_template_detail_tab
         WHERE  file_template_id = file_template_id_ ;
   BEGIN
      OPEN exist_control;
      FETCH exist_control INTO dummy_;
      IF (exist_control%NOTFOUND) THEN
         BEGIN
            OPEN get_rowno;
            FETCH get_rowno INTO row_no_;
            IF (get_rowno%NOTFOUND) THEN
               row_no_ := 0;
            END IF;
            CLOSE get_rowno;
            row_no_ := row_no_ + 1;
            INSERT INTO Ext_File_Template_Detail_Tab 
               (file_template_id,
                row_no, 
                file_type, 
                record_type_id, 
                column_id, 
                column_no, 
                start_position, 
                end_position, 
                denominator, 
                date_format, 
                control_column,
                hide_column,
                column_sort,
                max_length,
                rowversion)
            VALUES
               (file_template_id_,
                row_no_,
                file_type_, 
                record_type_id_, 
                column_id_, 
                column_no_, 
                start_position_, 
                end_position_, 
                denominator_, 
                date_format_, 
                control_column_,
                hide_column_,
                column_sort_,
                max_length_,
                SYSDATE);
         EXCEPTION
            WHEN dup_val_on_index THEN
               NULL;      
         END;
      END IF;
      CLOSE exist_control;
   END Attribute_Row_;
