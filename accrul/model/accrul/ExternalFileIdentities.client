client ExternalFileIdentities;
component ACCRUL;
layer Core;
projection ExternalFileIdentitiesHandling;

----------------------------- NAVIGATOR ENTRIES -----------------------------
navigator {
   entry ExternalFileIdentityNavEntry parent AccrulNavigator.MonitorExternalFilesMenu at index 300 {
      label = "External File Identities";
      page List home ExtFileIdentity;
   }
}


-------------------------------- MAIN PAGES ---------------------------------
page List using ExtFileIdentitySet {
   label = "External File Identities";

   list ExtFileIdentityList;
}


